{
  "name": "ant-design-boilerplate",
  "version": "1.0.0",
  "description": "ES6 React Ant-Design boilerplate",
  "main": "index.js",
  "engines": {
    "node": "v6.10.3",
    "npm": "5.6.0"
  },
  "scripts": {
    "start": "webpack-dev-server",
    "clean": "rm -rf dist",
    "production": "cross-env npm run clean && webpack --config webpack.production.config.js --progress --profile --colors",
    "lint": "eslint ./app/**/**.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "precommit": "cross-env npm run lint && npm test",
    "build": "NODE_ENV=production webpack --config ./webpack.production.config.js --progress --colors",
    "prepush": "cross-env npm run lint && npm test",
    "heroku-postbuild": "webpack -p --config ./webpack.production.config.js --progress"
  },
  "author": "Etor Diaz <etor.diaz.acha@gmail.com>",
  "license": "MIT",
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "modulePathIgnorePatterns": [
      "core"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "bail": true,
    "collectCoverageFrom": [
      "app/**/*.{js,jsx}"
    ]
  },
  "dependencies": {
    "antd": "^3.15.0",
    "babel-core": "^6.26.3",
    "babel-loader": "7.1.3",
    "babel-plugin-import": "^1.6.7",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-boilerplate": "1.1.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-2": "6.24.1",
    "babel-runtime": "^6.26.0",
    "copy-webpack-plugin": "4.3.0",
    "cross-env": "5.1.4",
    "css-loader": "0.28.11",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "3.0.2",
    "html-webpack-plugin": "2.30.1",
    "lodash.get": "^4.4.2",
    "prop-types": "^15.6.1",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-hot-loader": "4.0.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "4.2.2",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-saga": "^0.16.0",
    "reqwest": "^2.0.5",
    "reselect": "^3.0.1",
    "sass-loader": "6.0.7",
    "style-loader": "0.20.0",
    "webpack": "3.11.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "22.4.1",
    "css-hot-loader": "1.3.8",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "eslint": "4.15.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-loader": "2.0.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.6.0",
    "file-loader": "1.1.10",
    "husky": "0.15.0-rc.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "22.1.0",
    "jest-cli": "^22.4.3",
    "jest-css-modules": "^1.1.0",
    "node-sass": "^4.11.0",
    "open-browser-webpack-plugin": "0.0.5",
    "react-addons-test-utils": "15.6.2",
    "react-test-renderer": "16.2.0",
    "rimraf": "2.6.2",
    "url-loader": "1.0.0",
    "webpack-dev-server": "2.9.6"
  }
}
